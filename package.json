{
  "name": "person-detection-app",
  "version": "1.0.0",
  "description": "Full-stack Person Detection App with React frontend and Node.js backend using PyTorch",
  "main": "server/server.js",
  "homepage": ".",
  "scripts": {
    "start": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "server-install": "cd server && npm install",
    "build": "cd client && npm run build",
    "install-all": "npm run server-install && npm run client-install",
    "test": "cd test-scripts && python test_backend.py",
    "train": "cd running-scripts && python train_person.py",
    "detect": "cd running-scripts && python detect_person.py",
    "deploy:render": "cd server && npm run deploy:render",
    "deploy:vercel": "cd server && npm run deploy:vercel"
  },
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manishnegi120491/obejctIdentifyApp.git"
  },
  "keywords": [
    "person-detection",
    "pytorch",
    "react",
    "nodejs",
    "computer-vision",
    "ai"
  ],
  "author": "Manish Negi",
  "license": "MIT",
  "workspaces": [
    "client",
    "server"
  ]
}
